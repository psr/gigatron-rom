                    STW                     _decay                          ; if fade &&= decay then inc cIndex : fade = 0 : decay = decay LSL 1

_else_0x0455        LDI                     0
                    STW                     _x                              ; for x=0 to 159

_next_0x0459        LDW                     _y
                    ADDI                    8
                    STW                     0xc0
                    LD                      0xc0
                    ST                      _a + 1                          ; a = ((y + 8) LSL 8)

                    STW                     0xde
                    LDWI                    _page_0x0400
                    CALL                    giga_vAC
_page_0x0400        LDW                     0xde
                    LDW                     _a
                    ADDW                    _x
                    STW                     0xc0
                    LDWI                    0xff6c
                    ADDW                    _cIndex
                    ADDW                    _cIndex
                    DEEK                    
                    POKE                    0xc0                            ; poke a+x, sky(cIndex)

                    ForNextLoopUp           _x _next_0x0459 159             ; next x

                    LDW                     _y
                    ADDW                    _y
                    STW                     0xc0
                    LDWI                    505
                    STW                     register0
                    LDW                     0xc0
                    POKE                    register0                       ; poke &h01F9, y+y

                    ForNextLoopDown         _y _next_0x043b 0               ; next y

                    LDI                     3
                    ST                      waveType
                    LDWI                    -32768
                    PlayMidi                                                ; play midi &h8000,3

                    LDI                     0
                    STW                     _x
                    LDI                     8
                    STW                     _v                              ; x=0 : v=8

_loop               LDW                     _x
                    ANDI                    255
                    STW                     _i                              ; i = x AND 255

                    LDI                     0
                    STW                     _j                              ; j = 0

                    LDWI                    496
                    STW                     _q                              ; q = &h0100 + 240

                    LDI                     32
                    STW                     _yy                             ; yy = horizonY

                    LDWI                    _calcPitch
                    CALL                    giga_vAC                        ; gosub calcPitch

                    LDI                     0
                    STW                     _y
                    LDW                     _pitch
                    STW                     0xb0
                    LDI                     1
                    STW                     0xb2                            ; for y=0 to pitch

_next_0x04f6        LDW                     _j
                    ADDW                    _i
                    STW                     _j                              ; j = j + i

                    ForNextLoopStepUp       _y _next_0x04f6 0xb0 0xb2       ; next y

                    LDW                     _i
                    ADDW                    _i
                    STW                     _i                              ; i = i + i

                    LDWI                    321
                    STW                     _p                              ; p = &h0101 + horizonY + horizonY

                    LDI                     1
                    STW                     waitVBlankNum
                    WaitVBlank                                              ; wait 1

_repeat_0x0524      LD                      _j + 1
                    POKE                    _p                              ; poke p, j LSR 8

                    LDW                     _j
                    ANDI                    255
                    ADDW                    _i
                    STW                     _j                              ; j = i + (j AND 255)

                    LDW                     _p
                    ADDI                    4
                    STW                     _p
                    SUBW                    _q
                    BLT                     _repeat_0x0524                  ; p = p + 4 : until p &&>= q

                    LDI                     0x11
                    PEEK                    
                    STW                     numericLabel
                    LDI                     0
                    STW                     defaultLabel
                    LDWI                    gosubNumericLabel
                    CALL                    giga_vAC                        ; gosub peek(17)

                    LDW                     _x
                    ADDW                    _v
                    STW                     _x                              ; x = x + v

                    LDWI                    _loop
                    CALL                    giga_vAC                        ; goto loop

_254                PUSH                    
                    LDW                     _v
                    ADDI                    1
                    STW                     _v
                    SUBI                    32
                    BLE                     _else_0x0584
                    LDI                     32
                    STW                     _v                              ; v = v + 1 : if v &&> 32 then v = 32

_else_0x0584        POP                     
                    RET                                                     ; return

_253                PUSH                    
                    STW                     0xde
                    LDWI                    _page_0x0500
                    CALL                    giga_vAC
_page_0x0500        LDW                     0xde
                    LDW                     _v
                    SUBI                    1
                    STW                     _v
                    LDWI                    -32
                    SUBW                    _v
                    BLT                     _else_0x05a0
                    LDWI                    -32
                    STW                     _v                              ; v = v - 1 : if v &&< -32 then v = -32

_else_0x05a0        POP                     
                    RET                                                     ; return

_191                PUSH                    
                    LDI                     0
                    STW                     _v
                    POP                     
                    RET                                                     ; v = 0 : return

_127                PUSH                    
                    LDI                     0
                    STW                     _v
                    POP                     
                    RET                                                     ; v = 0 : return

_calcPitch          PUSH                    
                    LDWI                    -23
                    ADDW                    _yy
                    STW                     _pitch                          ; pitch = yy + (96 - 120 + 1)

                    LDW                     _pitch
                    ADDI                    1
                    STW                     0xc0
                    LDWI                    0xfffe
                    ANDW                    0xc0
                    STW                     _pitch                          ; pitch = (pitch + 1) AND &hfffe

                    POP                     
                    RET                                                     ; return

_end_0x05cb         BRA                     _end_0x05cb                     ; END



